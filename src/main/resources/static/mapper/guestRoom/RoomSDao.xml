    <!--房间start-->
    <select id="queryRoomDao" resultType="java.util.Map">
        SELECT b.floorname,a.* FROM  GS_ROOM a,gs_floors b where a.floorcode=b.floorcode
    </select>

    <select id="queryFloorRoomMapper"  resultType="java.util.Map">
        SELECT DISTINCT a.id,a.floorname,b.* from FLOOR a,ROOM b where  a.floorcode=b.floorcode
    </select>


